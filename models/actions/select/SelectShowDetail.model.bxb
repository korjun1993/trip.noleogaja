action (SelectShowDetail) {
  description (상세 보기)
  type(Fetch)
  collect {
    input (contentId) {
      type (ContentId)
      min (Required){
        on-empty {
          halt{
            dialog {
              template-macro (NotFoundWhatIsFestival)
            }
          }
        }
      } 
      max (One)
    }
  }
  output (ShowDetail)
}
