action (SelectShowDetail) {
  description (상세 보기)
  type(Fetch)
  collect {
    input (contentId) {
      type (ContentId)
      min (Required) max (One)
      default-init {
        if (!exists(contentId)) {
          intent {
            goal: FindFestivals
          }
        }
      }
    }
  }
  output (ShowDetail)
}
