result-view{
  match : FestivalList(this)

  message{
    template ("에서 진행되고 있는 행사는 입니다 #{size(this)}")
  }

  render{
    list-of (this){
      where-each (item){
        layout-macro (festival-result) {
          param (festivalList) {
            expression(item)
          }
        }
      }
    }
  }

  conversation-drivers{
    conversation-driver{
      template ("태그 test")
    }
  }
}