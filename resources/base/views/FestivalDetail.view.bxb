result-view{
	match : ShowDetail(this)
	
	message{
		template ("")
	}
	
	render{
		layout{
			section{
				content{
					image-carousel {
						images (this.images)
					}
					title-area {
						halign (Center)
						slot1 {
							text {
								value ("#{value(this.title)}")
								style (Title_L)
							}
						}
						slot2{
							paragraph{
								value("#{value(this.addr1)}")
								style(Title_XS)
							}
						}
						slot3{
							paragraph{
								value("#{value(this.eventStartDate)} ~ #{value(this.eventEndDate)}")
								style(Title_XS)
							}
						}
					}
				}
			}
			section{
				content{
					if (this.eventPlace != ' '){
						paragraph{
							value("행사 장소")
							style(Legal)
						}
						paragraph{
							value("#{value(this.eventPlace)}")
						}
					}
					spacer
					if(this.playTime != ' '){
						paragraph{
							value("행사 시간")
							style(Legal)
						}
						paragraph{
							value("#{value(this.playTime)}")
						}
					}
					spacer
					if(this.ageLimit != ' '){
						paragraph{
							value("연령 제한")
							style(Legal)
						}
						paragraph{
							value("#{value(this.ageLimit)}")
						}
					}
					spacer
					if(this.useTimeFestival != ' '){
						paragraph{
							value("요금 정보")
							style(Legal)
						}
						paragraph{
							value("#{value(this.useTimeFestival)}")
						}
					}
					spacer
					if(this.discountInfoFestival != ' '){
						paragraph{
							value("할인 정보")
							style(Legal)
						}
						paragraph{
							value("#{value(this.discountInfoFestival)}")
						}
					}
					spacer
					if(this.placeInfo != ' '){
						paragraph{
							value("오시는 길")
							style(Legal)
						}
						paragraph{
							value("#{value(this.placeInfo)}")
						}
					}
					spacer
					if (this.subEvent != ' '){
						paragraph{
							value("세부 이벤트")
							style(Legal)
						}
						paragraph{
							value("#{value(this.subEvent)}")
						}
					}
					spacer
					if(this.homePage != ' '){
						paragraph{
							value("홈페이지")
							style(Legal)
						}
						cell-card{
							slot2{
								content{
									primary{
										template ("#{value(this.homePage)}")
									}
								}
							}
							on-click{
								intent{
									goal: OpenBrowser
									value: $expr(this.homePage)
								}
							}
						}
					}
					spacer
					if(this.telName != ' ' && this.tel != ' '){
						paragraph{
							value("문의")
							style(Legal)
						}
						cell-card{
							slot2{
								content{
									primary{
										template ("#{value(this.telName)} : #{value(this.tel)}")
									}
								}
							}
							on-click{
								intent{
									goal: OpenBrowser
									value: $expr(this.tel)
								}
							}
						}
					}
					spacer
					if(this.point.latitude != ' ' && this.point.longitude != ' '){
						map-card{
							aspect-ratio(16:9)
							title-area{
								slot1{
									single-line {
										text {
											style(Title_L)
											color (Amber)
											value("")
										}
									}
								}
							}
							markers{
								marker{
									geo (this.point)
									icon("/img/marker.png")
									width (35)
									height (40)
									anchor-x (5)
									anchor-y (15)
									rotation (0)
								}
							}
							zoom(16)
							on-click{
								intent{
									goal: OpenBrowser
									value: $expr(this.point)
								}
          		}
						}
					}
				}
			}
		}
	}

	conversation-drivers{
    conversation-driver{
      template ("유튜브에서 찾아줘")
    }
  }
}