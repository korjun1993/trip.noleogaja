      //     value: Festivals{
      //       contentId : $expr(festivals.contentId)
      //       eventStartDate : $expr(festivals.eventStartDate)
      //       eventEndDate : $expr(festivals.eventEndDate)
      //       title : $expr(festivals.title)
      //       outputLocation : $expr(festivals.outputLocation)
      //     }
      //   }
      // }
    }
  }
}